{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "markup",
          "bash",
          "python"
        ],
        "plugins": [
          "line-numbers",
          "toolbar",
          "copy-to-clipboard"
        ],
        "theme": "twilight",
        "css": true
      }
    ]
  ]
}